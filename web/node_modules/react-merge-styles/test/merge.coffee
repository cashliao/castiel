chai  = require 'chai'
merge = require '../src/'

chai.should()

describe 'Kitchen Sink', =>
  it 'passes', =>
    merge(
      a : 10
      b :
        c : 20
        d :
          e : 30
        f :
          a : 40
          g : 50
          h :
            c : 60
    ).should.deep.equal(
      a : 10
      b :
        a : 10
        c : 20
        d :
          a : 10
          c : 20
          e : 30
        f :
          a : 40
          c : 20
          g : 50
          h :
            a : 40
            c : 60
            g : 50
    )
